        <dt class="col-sm-2">
            Especialidad
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ProjectFinished.Specialty)
        </dd>
        <dt class="col-sm-2">
            Nivel
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ProjectFinished.Level)
